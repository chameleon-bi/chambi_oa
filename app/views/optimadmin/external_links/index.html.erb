<%= render 'optimadmin/shared/header/main_title', title: 'Manage External Links' %>

<div class="main-content">
  <%= render 'optimadmin/shared/module/form/back', path: menu_items_path, title: 'Manage Menu', prefix: 'Back to' %>

  <%= render 'optimadmin/shared/module/index/new_item', title: 'External Link', object: Optimadmin::ExternalLink, path: new_external_link_path %>

  <div class="row collapse module-list-index header">
    <%= list_item 'large-10 small-5' do 'Link' end %>

    <%= list_item { 'Edit' } %>

    <%= list_item 'large-1 hide-for-medium-down text-center last' do 'Delete' end %>
  </div>


  <%= render_object @external_links %>
</div>
